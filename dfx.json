{
  "canisters": {
    "usdb-rune-backend": {
      "candid": "src/usdb-rune-backend/usdb-rune-backend.did",
      "package": "usdb-rune-backend",
      "type": "rust"
    },
    "usdb-rune-frontend": {
      "dependencies": [
        "usdb-rune-backend"
      ],
      "source": [
        "src/usdb-rune-frontend/dist"
      ],
      "type": "assets",
      "workspace": "usdb-rune-frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "--release"
    }
  },
  "output_env_file": ".env",
  "version": 1
}
